public class MySwitch {  
	public static void main(String[] args) {
//		double score = 98;  //����
		int score = 98;
		switch(score/10) {
		case 10:
			System.out.println("niubi");
			break;
		case 9:  //9.0~9.9����ȡֵ9
			System.out.println("999");
			break;
		default:
			System.out.println("caodan ");
		}
	}
}
