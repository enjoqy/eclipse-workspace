package cn.kgc.demo02;

public class DemoSystem {
	
	public static void main(String[] args) {
		nijie nijie = null;
		for (int i = 0; i < 100; i++) {
		nijie = new nijie();
		System.gc();
		}
	}

}

class nijie {

	@Override
	protected void finalize() throws Throwable {
		System.out.println("我被回收了！");
		super.finalize();
	}
	
}
