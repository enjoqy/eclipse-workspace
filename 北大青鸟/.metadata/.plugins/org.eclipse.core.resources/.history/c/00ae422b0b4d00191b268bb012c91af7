package Demo;

import java.util.jar.Attributes.Name;

import org.apache.log4j.Logger;

import com.mysql.jdbc.log.Log;

public class Test02 {
	
	private String str = "11";
	private int age = 1;
	
	
	
	
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime*result + ((str == null)?0:str.hashCode());
		result = prime*result + age;
		return result;
		
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Test02 other = (Test02) obj;
		if (age != other.age)
			return false;
		if (str == null) {
			if (other.str != null)
				return false;
		} else if (!str.equals(other.str))
			return false;
		return true;
	}

	public static Logger log = Logger.getLogger(Test02.class); 
	
	public static void main(String[] args) {
		
		SuperMan sm = new SuperMan();
		SuperMan sm1 = new SuperMan();
		
		System.out.println(sm == sm1);
		System.out.println(sm.equals(sm1));
		
		log.debug("result: " + (sm.equals(sm1)));
		
		
		Demo01<String , Integer, Character> d = new Demo01<String, Integer, Character>();
		
		d.setName("11");
		d.setAge(11);
		d.setSex('1');
		System.out.println(d);
		log.debug(d);
		
		String str = "Hello World";
		
		System.out.println(str.hashCode());
		System.out.println(Test01.class);
		
	}
	
	

}
