package Demo;

import org.apache.log4j.Logger;

import com.mysql.jdbc.log.Log;

public class Test02 {
	
	public static Logger logger= Logger.getRootLogger();
	
	public static void main(String[] args) {
		
		Logger log = Logger.getLogger(Test02.class); 
		
		
		
		SuperMan sm = new SuperMan();
		SuperMan sm1 = new SuperMan();
		
		logger.debug(sm1 = sm);
		
		System.out.println(sm == sm1);
		System.out.println(sm.equals(sm1));
		
		
		
		Demo01<String , Integer, Character> d = new Demo01<String, Integer, Character>();
		
		d.setName("11");
		d.setAge(11);
		d.setSex('1');
		System.out.println(d);
		log.debug(d);
		
		
		
	}

}
